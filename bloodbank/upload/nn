def add(a,b):
    return a+b
def multiply(a,b):
    return a*b
def communicate(operation,a,b):
    if(operation=='add'):
        return add(a,b)
    elif(operation=='multiply'):
        return multiply(a,b)
    else:
        return None
result1=communicate('add',2,3)
result2=communicate('multiply',4,5)
print(result1)
